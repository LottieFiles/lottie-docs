{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Rounded Corners",
    "description": "Rounds corners of other shapes",
    "allOf": [
        {
            "$ref": "#/$defs/shapes/modifier"
        },
        {
            "type": "object",
            "properties": {
                "ty": {
                    "title": "Shape Type",
                    "type": "string",
                    "const": "rd"
                },
                "r": {
                    "title": "Radius",
                    "$ref": "#/$defs/properties/scalar-property"
                }
            },
            "required": [
                "ty", "r"
            ]
        }
    ]
}
