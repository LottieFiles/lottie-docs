{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Text Range Selector",
    "properties": {
        "t": {
            "title": "Expressible",
            "$ref": "#/$defs/values/int-boolean"
        },
        "xe": {
            "title": "Max Ease",
            "$ref": "#/$defs/properties/scalar-property"
        },
        "ne": {
            "title": "Min Ease",
            "$ref": "#/$defs/properties/scalar-property"
        },
        "a": {
