{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "Null Layer",
    "description": "Layer with no data, useful to group layers together",
    "allOf": [
        {
            "$ref": "#/$defs/layers/visual-layer"
        },
        {
            "type": "object",
            "properties": {
                "ty": {
                    "title": "Type",
                    "description": "Layer type",
                    "type": "integer",
                    "const": 3
                }
            },
            "required": [
                "ty"
            ]
        }
    ]
}
